{
  "version": 2,
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "dist/index.cjs",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 60
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/index.cjs"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/dist/public/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.cjs"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/dist/index.cjs"
    }
  ]
}
